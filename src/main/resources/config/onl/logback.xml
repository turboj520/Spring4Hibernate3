	<root>
		<level value="INFO" />
		<appender-ref ref="asyncAll" />
	</root>

	<logger name="com.athm">
		<level value="DEBUG" />
		<appender-ref ref="stdout"></appender-ref>
	</logger>

	<logger name="com.athm.framework.cache">
		<level value="INFO" />
	</logger>

	<!-- 需要记录日志的包 -->
	<logger name="org.springframework">
		<level value="WARN" />
	</logger>

	<logger name="org.hibernate.type.descriptor.sql.BasicBinder" additivity="false">
		<level value="${log.sql.parameter.trace}" />
		<appender-ref ref="sqlFile" />
		<appender-ref ref="stdout"></appender-ref>
	</logger>

	<logger name="org.hibernate.SQL" additivity="false">
		<level value="${log.sql.parameter.trace}" />
		<appender-ref ref="sqlFile" />
		<appender-ref ref="stdout"></appender-ref>
	</logger>
	<logger name="org.hibernate.cache">
		<level value="WARN" />
	</logger>

	<logger name="org.hibernate">
		<level value="WARN" />
	</logger>

