<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="false">
	<property resource="properties/logback.properties" />
	<jmxConfigurator />
	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%date [%thread] %-5level %logger{30} %M %L - %msg%n
			</pattern>
		</encoder>
	</appender>

	<appender name="sqlFile" class="ch.qos.logback.core.rolling.RollingFileAppender">
		<file>${sql.log.file.name}.log</file>
		<rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
			<fileNamePattern>${sql.log.file.name}.%d{yyyy-MM-dd}.log
			</fileNamePattern>
			<!-- keep 30 days' worth of history -->
			<maxHistory>180</maxHistory>
		</rollingPolicy>
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{80} - %msg%n</pattern>
		</encoder>
	</appender>

	<appender name="dealerPwdFile" class="ch.qos.logback.core.rolling.RollingFileAppender">
		<file>${dealerpwd.log.file.name}.log</file>
		<rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
			<fileNamePattern>${dealerpwd.log.file.name}.%d{yyyy-MM-dd}.log
			</fileNamePattern>
			<!-- keep 30 days' worth of history -->
			<maxHistory>180</maxHistory>
		</rollingPolicy>
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{80} - %msg%n</pattern>
		</encoder>
	</appender>

	<appender name="smsfail" class="ch.qos.logback.core.rolling.RollingFileAppender">
		<file>${smsfail.log.file.name}.log</file>
		<rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
			<!-- rollover daily -->
			<fileNamePattern>${smsfail.log.file.name}.%d{yyyy-MM-dd}.%i.log
			</fileNamePattern>
			<timeBasedFileNamingAndTriggeringPolicy class="ch.qos.logback.core.rolling.SizeAndTimeBasedFNATP">
				<!-- or whenever the file size reaches 100MB -->
				<maxFileSize>20mb</maxFileSize>
			</timeBasedFileNamingAndTriggeringPolicy>
			<!-- keep 30 days' worth of history -->
			<maxHistory>180</maxHistory>
		</rollingPolicy>
		<encoder>
			<pattern>%date %-5level [%-20thread] %logger{20} - %msg%n</pattern>
		</encoder>
	</appender>

	<appender name="homeOrderPushFailAppender" class="ch.qos.logback.core.rolling.RollingFileAppender">
		<file>${homeOrderPushFail.log.file.name}.log</file>
		<rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
			<!-- rollover daily -->
			<fileNamePattern>${homeOrderPushFail.log.file.name}.%d{yyyy-MM-dd}.%i.log
			</fileNamePattern>
			<timeBasedFileNamingAndTriggeringPolicy class="ch.qos.logback.core.rolling.SizeAndTimeBasedFNATP">
				<!-- or whenever the file size reaches 100MB -->
				<maxFileSize>100mb</maxFileSize>
			</timeBasedFileNamingAndTriggeringPolicy>
			<!-- keep 180 days' worth of history -->
			<maxHistory>180</maxHistory>
		</rollingPolicy>
		<encoder>
			<pattern>%date %-5level [%-20thread] %logger{20} [%L] - %msg%n</pattern>
		</encoder>
	</appender>

	<appender name="unPayOrderInvalidFailAppender" class="ch.qos.logback.core.rolling.RollingFileAppender">
		<file>${unPayOrderInvalidFail.log.file.name}.log</file>
