	</logger>

	<!-- 短信发送失败专用日志 -->
	<logger name="asyncSmsfailLOGGER">
		<level value="INFO" />
		<appender-ref ref="smsfail" />
	</logger>

	<!-- 上门订单推送失败日志 -->
	<logger name="homeOrderPushFailLogger" additivity="false">
		<level value="INFO" />
		<appender-ref ref="homeOrderPushFailAppender" />
	</logger>

	<logger name="unPayOrderInvalidFailLogger" additivity="false">
		<level value="INFO" />
		<appender-ref ref="unPayOrderInvalidFailAppender" />
	</logger>

	<!-- Session信息记录日志 -->
	<logger name="sessionLogger">
		<level value="INFO" />
		<appender-ref ref="sessionAppender" />
	</logger>

	<logger name="DataTransportWork" additivity="false">
		<level value="trace" />
		<appender-ref ref="dataTransport" />
	</logger>

	<logger name="BizSignOn" additivity="false">
		<level value="trace" />
