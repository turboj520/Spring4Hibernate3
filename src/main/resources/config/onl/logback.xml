	<appender name="allFile" class="ch.qos.logback.core.rolling.RollingFileAppender">
		<file>${all.log.file.name}.log</file>
		<rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
			<!-- rollover daily -->
			<fileNamePattern>${all.log.file.name}.%d{yyyy-MM-dd}.%i.log
			</fileNamePattern>
			<timeBasedFileNamingAndTriggeringPolicy class="ch.qos.logback.core.rolling.SizeAndTimeBasedFNATP">
				<!-- or whenever the file size reaches 100MB -->
				<maxFileSize>200mb</maxFileSize>
			</timeBasedFileNamingAndTriggeringPolicy>
			<!-- keep 30 days' worth of history -->
			<maxHistory>180</maxHistory>
		</rollingPolicy>

		<encoder>
			<pattern>%d{HH:mm:ss.SSS} %-5level [%-20thread] %logger{20} - %msg%n</pattern>
		</encoder>
	</appender>

	<appender name="asyncAll" class="ch.qos.logback.classic.AsyncAppender">
		<queueSize>10240</queueSize>
		<appender-ref ref="allFile" />
	</appender>

	<appender name="dataTransport" class="ch.qos.logback.core.rolling.RollingFileAppender">
